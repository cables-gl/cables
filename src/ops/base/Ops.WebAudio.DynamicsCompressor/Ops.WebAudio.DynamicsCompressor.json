                "text": "The audio signal you want to compress"
            },
            {
                "name": "threshold",
                "text": "How loud the signal has to be before compression is applied."
            },
            {
                "name": "knee",
                "text": "Sets how the compressor reacts to signals once the threshold is passed."
            },
            {
                "name": "ratio",
                "text": "How much compression is applied. For example, if the compression ratio is set for 6:1, the input signal will have to cross the threshold by 6 dB for the output level to increase by 1dB."
            },
            {
                "name": "attack",
                "text": "How quickly the compressor starts to work."
            },
            {
                "name": "release",
                "text": "How soon after the signal dips below the threshold the compressor stops."
            },
            {
                "name": "exe",
                "text": "Only needed to update the `Reduction` port, has no effect on the compression"
            },
            {
                "name": "audio out",
